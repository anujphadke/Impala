---- QUERY
# alltypes has 7300 rows - codegen should be disabled regardless
# of # of backend impala daemons.
set disable_codegen_rows_threshold=8000;
select count(*) from alltypes t1
  join alltypestiny t2 on t1.id = t2.id
---- RESULTS
8
